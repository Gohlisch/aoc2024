<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\AoCUtils\AoCUtils.csproj" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="Puzzle.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Day01Puzzle.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="Puzzle.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>Puzzle.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
